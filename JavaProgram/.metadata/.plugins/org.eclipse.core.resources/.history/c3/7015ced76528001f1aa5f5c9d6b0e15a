package test;

public class Test6_Student {
    String name; // 학생 이름
    int ban;     // 번호
    int kor;     // 국어 점수
    int eng;     // 영어 점수
    int math;    // 수학 점수

    // 생성자
    public Test6_Student(String name, int ban, int kor, int eng, int math) {
        this.name = name;
        this.ban = ban;
        this.kor = kor;
        this.eng = eng;
        this.math = math;
    }
}
